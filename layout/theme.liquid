<!doctype html>
<!--[if lt IE 7]><html class="no-js lt-ie10 lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]><html class="no-js lt-ie10 lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]><html class="no-js ie8 lt-ie10 lt-ie9" lang="en"> <![endif]-->
<!--[if IE 9 ]><html class="no-js ie9 lt-ie10"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> 
<html class="no-js"> <!--<![endif]-->
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

  <title>
    {{ page_title }}

    {% if current_tags %}
      &#8211; {{ current_tags | join: ', ' }}
    {% endif %}

    {% if current_page != 1 %}
      &#8211; {{ 'general.pagination.page' | t: page_number: current_page }}
    {% endif %}

    {% unless page_title contains shop.name %}
      &#8211; {{ shop.name }}
    {% endunless %}
  </title>

  {% if page_description %}
    <meta name="description" content="{{ page_description | escape }}" />
  {% endif %}

 <link rel="shortcut icon" href="{{ 'favicon.ico' | asset_url }}" type="image/x-icon" />


  {% if template contains 'collection' and current_tags %}
    <meta name="robots" content="noindex" />
    <link rel="canonical" href="{{ shop.url }}{{ collection.url }}" />
  {% else %}
    <link rel="canonical" href="{{ canonical_url }}" />
  {% endif %}
  <meta name="viewport" content="width=device-width,initial-scale=1" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">

	  <!-- royal slider -->
  {{ 'royalslider.css' | asset_url | stylesheet_tag }} 
  {{ 'rs-minimal-white.css' | asset_url | stylesheet_tag }}
  
   <!-- Scripts -->
  {{ 'jquery-1.11.1.min.js' | asset_url | script_tag }}
  {{ 'api.jquery.js' | shopify_asset_url | script_tag }}
  {{ 'text.js' | asset_url | script_tag }}
  {% render 'orbislabs_sitemap_noindex' %}
      <!-- CSS bootstrap -->
  {{ 'bootstrap-grid.css' | asset_url | stylesheet_tag }}

  <!-- Theme CSS -->
  {{ 'theme.scss.css' | asset_url | stylesheet_tag }}

	
  <!-- Swatch CSS   -->	
  {{ 'swatch.css' | asset_url | stylesheet_tag }}
  
  <!--  Custom CSS  -->
  {{ 'custom.scss.css' | asset_url | stylesheet_tag }}
  
  {{ content_for_header }}

    {% include 'social-meta' %}

  
  {% if settings.custom-css != blank %}
    <style>
      {{ settings.custom-css }}
    </style>
  {% endif %}

  <!-- Third Party JS Libraries -->
  {{ 'modernizr-2.8.2.min.js' | asset_url | script_tag }}

  <!-- Theme object -->
  {% include 'theme-options' %}

  <script>
    window.products = {};
  </script>

</head>

<body class="{% if customer %}customer-logged-in{% endif %} template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}  {% if settings.blog-promo %}has-promo{% endif %} {% if settings.related-products and collection.products.size > 1 and template contains 'product' %}has-related-products-{{ settings.related-products-position }}{% endif %} {% if settings.product-hover == 'quick-shop' %}has-quick-shop{% endif %} loading" >

   {% if settings.product-hover == 'quick-shop' %}
  {% include 'quick-shop' %}
  {% endif %}

  {% section 'header' %}
  

  {% if template == 'index' and settings.slideshow %}
  {% include 'home-slideshow' %}
  {% endif %}
  
  {% comment %}

  {% if template == 'index' and settings.masonry-features %}
  {% include 'masonry-features' %}
  {% endif %}
  
  {% endcomment %}

  {% if template == 'index' %}
  {% if settings.featured-collections or settings.featured-products or settings.instagram-widget or settings.twitter-widget %}
  <div class="main-content">
    {{ content_for_layout }}
  </div>
  {% endif %}
  {% else %}
  <div class="main-content">
    {{ content_for_layout }}
  </div>
  {% endif %}
 {% section 'footer' %}
<script>
jQuery(function() {
  jQuery('.swatch :radio').change(function() {
    var optionIndex = jQuery(this).closest('.swatch').attr('data-option-index');
    var optionValue = jQuery(this).val();
    jQuery(this)
      .closest('form')
      .find('.single-option-selector')
      .eq(optionIndex)
      .val(optionValue)
      .trigger('change');
  });
});
</script>
  
  <script type="text/javascript">
/* <![CDATA[ */
var google_conversion_id = 939199686;
var google_custom_params = window.google_tag_params;
var google_remarketing_only = true;
/* ]]> */
</script>
<script type="text/javascript" src="//www.googleadservices.com/pagead/conversion.js">
</script>
<noscript>
<div style="display:inline;">
<img height="1" width="1" style="border-style:none;" alt="" src="//googleads.g.doubleclick.net/pagead/viewthroughconversion/939199686/?value=0&amp;guid=ON&amp;script=0"/>
</div>
</noscript>
  {% if template contains 'cart' %}
    {{ "/services/javascripts/countries.js" | script_tag }}
    {{ "shopify_common.js" | shopify_asset_url | script_tag }}
    {{ 'jquery.cart.min.js' | asset_url | script_tag }}
  {% endif %}

  {% if template contains 'customers' %}
    {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
    {{ 'customer_area.js'  | shopify_asset_url | script_tag }}
  {% endif %}

  {% if settings.enable_currency_converter %}
    {{ '//cdn.shopify.com/s/javascripts/currencies.js' | script_tag }}
  {{ "jquery.currencies.js" | asset_url | script_tag }}
  {% endif %}.
    

<!-- wish-list -->
  {{ 'wish-list.js' | asset_url | script_tag }}
  <!-- end wish list -->
  {{ 'plugins.js'  | asset_url | script_tag }}
   {{ 'theme.js'  | asset_url | script_tag }}
  {{ 'grid.js'  | asset_url | script_tag }}

<!-- royal slider js -->
  {{ 'jquery.browser.js' | asset_url | script_tag }}
  {{ 'jquery.royalslider.min.js' | asset_url | script_tag }}
  
  <!-- selectButtons -->
  {{ 'selectButtons.js' | asset_url | script_tag }}
  {{ 'option_selection.js' | shopify_asset_url | script_tag }}
  <script>
  jQuery(document).ready(function($) {
    $('.slider1').royalSlider({
      arrowsNav: true,
      arrowsNavAutoHide: false,
      fadeinLoadedSlide: false,
      controlNavigationSpacing: 0,
      controlNavigation: 'bullets',
      imageScaleMode: 'none',
      imageAlignCenter:false,
      blockLoop: true,
      loop: true,
      numImagesToPreload: 6,
      transitionType: 'fade',
      keyboardNavEnabled: true,
      autoScaleSliderHeight: 450,     
      block: {
        delay: 400
      }
    });

    });
    
     var selectCallback = function(variant,selector){
      if (variant) {
          var form = jQuery('#' + selector.domIdPrefix).closest('form');
          for (var i=0,length=variant.options.length; i<length; i++) {
              var radioButton = form.find('.swatch[data-option-index="' + i + '"] :radio[value="' + variant.options[i] +'"]');
              if (radioButton.size()) {
                  radioButton.get(0).checked = true;
              }
          }
      }
  };
    
   
  </script>
  <!-- end slider js -->

</body>
</html>