      .post-content {
        position: relative;
        z-index: 500;
        display: block;
        margin: 0 auto;
        max-width: 560px;
        width: 100%;
      }

      .post-title {
        text-align: center;
        font-size: 36px;

        {% if settings.heading-font-small-caps %}
        font-size: 32px;
        {% endif %}

        a {
          color: $heading-color;
        }

        .has-featured-image & {
          background: $body-background-color;
          padding: 45px 45px 0;

          @include breakpoint(m) {
            padding: 25px 0 0;
          }
        }
      }

      .post-date {
        margin-top: 27px;
        margin-bottom: 30px;
        text-align: center;
        color: $meta-color;

        a {
          color: $meta-color;
        }
      }

      .post-content {
        .highlight {
          position: static;
        }

        .full-width {
          position: relative;
          max-width: none;

          @include breakpoint(m) {
            max-width: 705px !important;
            left: 50% !important;
            margin-left: -352.5px !important;
          }

          @include breakpoint(s) {
            max-width: 100% !important;
            left: 0 !important;
            margin-left: 0 !important;
          }
        }
      }

      .post-meta {
        margin: 60px 0 0;
        text-align: center;

        .share-buttons { margin-bottom: 20px; }

        .post-tags {
          margin-bottom: 40px;

          a {
            color: $meta-color;
          }

          .title {
            margin-right: 5px;
            color: $heading-color;
          }
        }
      }

      .post-author-avatar {
        vertical-align: middle;
        border-radius: 50%;
        width: 75px;
        margin-right: 25px;
      }

      .post-author-name {
        display: inline-block;
        vertical-align: middle;
        font-size: 16px;
        color: $meta-color;
        text-align: center;

        {% if settings.article-author-avatar %}
        text-align: left;
        {% endif %}

        &:before {
          @extend %meta-text;
          content: attr(data-title);
          display: block;
          color: $heading-color;
          font-size: 13px;

          {% if settings.meta-font-small-caps %}
          font-size: 12px;
          {% endif %}
        }
      }

      .share-buttons,
      .single-post-pagination,
      .post-comments {
        max-width: 560px;
        margin: 0 auto;
      }

      .single-post-pagination {
        margin-bottom: 40px;
        font-family: "chiko-icons";
      }

      .post-comments {
        padding-top: 30px;
        border-top: 1px solid $border-color;

        .pagination {
          margin: 0;
        }
      }

      .post-comments-list {
        padding-bottom: 40px;
        border-bottom: 1px solid $border-color;
      }

      .post-comment {
        padding-bottom: 40px;
        margin-bottom: 40px;
        border-bottom: 1px solid $border-color;

        &.last {
          border-bottom: 0;
          padding-bottom: 0;
          margin-bottom: 0;
        }
      }

      .post-comment-date {
        @extend %meta-text;
        display: block;
        margin-bottom: 20px;
      }

      .post-comment-details {
        margin-top: 20px;
      }

      .post-comment-author-avatar {
        margin-right: 10px;
        width: 40px;
        border-radius: 50%;
        vertical-align: middle;
      }

      .post-comment-author-name {
        color: $heading-color;
        vertical-align: middle;
      }

      .post-comments-submit {
        padding-top: 30px;

        label {
          display: block;
          margin-top: 32px;
          margin-bottom: 22px;
        }

        input[type=submit] {
          margin-top: 30px;
        }
      }


      .template-cart {
        .main-content {
          margin-bottom: 65px;
        }

        .empty {
          padding: 0 30px;
          text-align: center;
        }
      }

      .cart-form {
        max-width: $max-width;
        margin: 40px auto;
        padding: 0 30px;
      }

      .cart-items {
        width: 100%;
        border-top: 1px solid $border-color;
        @include transition(height 0.3s ease-in);

        &.empty {
          height: 0;
          overflow: hidden;
        }

        thead {
          @include breakpoint(s) {
            display: none;
          }
        }

        th,
        td {
          padding: 30px;
          border: 0;
        }

        th {
          padding: 20px 30px 20px 0;
          font-size: 12px;
          text-align: right;
          border-bottom: 1px solid $border-color;

          &.first {
            text-align: left;
            padding-left: 30px;
          }
        }
      }

      .cart-item {
        @include transition(opacity 0.3s ease-in, height 0.3s ease-in);

        &.removing {
          opacity: 0;
        }

        .removed { text-align: center; }

        @include breakpoint(s) {
          &.first { 
            .product-item,
            .remove {
              border-top: 0;
            }
          }
        }

        .product-item {
          position: relative;
          width: 55%;
          font-size: 0;
          padding-right: 0;

          @include breakpoint(s) {
            display: block;
            border-top: 1px solid $border-color;
            width: 100%;
            padding: 30px 30px 12px;

            .lt-ie10 & {
              float: left;
            }
          }
        }

        .image,
        .product-item-details {
          display: inline-block;
          vertical-align: middle;
          font-size: $base-font-size;
        }

        .image {
          width: 25%;

          @include breakpoint(s) {
            width: 50%;
          }

          img {
            @include breakpoint(s) {
              float: right;
            }
          }
        }

        .product-item-details {
          width: 75%;
          padding: 0 30px 0 20px;

          @include breakpoint(s) {
            width: 50%;
          }

          span {
            display: block;
          }
        }

        .price,
        .quantity,
        .total {
          width: 15%;
          padding-left: 0;
          text-align: right;

          &:before { @extend %meta-text; }

          @include breakpoint(s) {
            display: block;
            width: 100%;
            padding: 5px 30px;
            text-align: center;

            .lt-ie10 & {
              float: left;
            }

            &:before {
              display: inline-block;
              text-align: right;
              margin-right: 20px;
              color: $heading-color;
            }
          }
        }

        @include breakpoint(s) {
          .price:before,
          .quantity:before,
          .total:before {
            content: attr(data-title);
          }

          .total { padding-bottom: 30px; }
        }

        .quantity input {
          display: inline-block;
          width: 50px;
          text-align: center;
        }

        .remove {
          @extend %icon;
          position: absolute;
          top: 30px;
          left: 10px;
          font-size: 10px;
          color: $meta-color;
          -webkit-font-smoothing: antialiased;

          @include breakpoint(s) { top: 12px; }
        }
      }

      .cart-brand {
        @extend %meta-text;
        font-size: 12px;

        {% if settings.meta-font-small-caps %}
        font-size: 11px;
        {% endif %}

        a {
          color: $meta-color;
        }
      }

      .cart-title {
        margin: 5px 0;
        font-size: 16px;
      }

      .cart-undo {
        cursor: pointer;
        color: $accent-color;
      }

      .cart-tools {
        margin-top: 40px;
        font-size: 0;
      }

      .cart-instructions,
      .cart-totals {
        display: inline-block;
        vertical-align: top;
        font-size: $base-font-size;
      }

      .cart-instructions {
        width: 50%;

        @include breakpoint(s) {
          width: 100%;
        }

        h2 {
          margin: 0 0 20px;
          font-size: 14px;
        }

        textarea {
          width: 100%;
          max-width: 100%;
        }
      }

      .cart-totals {
        text-align: right;

        {% if settings.cart-special-instructions %}
        padding-left: 40px;
        width: 50%;
        {% else %}
        width: 100%;
        {% endif %}

        @include breakpoint(s) {
          text-align: left;
          margin-top: 40px;
          width: 100%;
          padding: 0;
        }
      }

      .cart-price {
        margin: 0;
        line-height: 1;
        font-size: 30px;
        font-family: $heading-font;
        color: $heading-color;
      }

      .cart-message {
        margin: 8px 0 0;
      }

      .cart-checkout {
        margin-top: 20px;
      }

      .cart-shipping-calculator {
        max-width: $max-width;
        margin: 0 auto;
        padding: 0 30px;
        font-size: 0;

        h2 {
          margin: 0 0 20px;
          font-size: 14px;
        }

        label {
          display: block;
          margin: 20px 0 10px;
        }

        .get-rates {
          margin-top: 24px;
        }

        .wrapper-setup,
        .cart-shipping-calculator-response {
          display: inline-block;
          vertical-align: top;
          font-size: $base-font-size;

          @include breakpoint(xs) {
            width: 100%;
            padding: 0;
          }
        }

        .wrapper-setup {
          width: 340px;

          @include breakpoint(m) {
            width: 300px;
          }

          @include breakpoint(s) {
            max-width: 300px;
          }
        }
      }

      .cart-shipping-calculator-response {
        padding-left: 40px;
        width: 400px;

        @include breakpoint(m) {
          width: 370px;
        }

        @include breakpoint(s) {
          padding-left: 0;
          max-width: 370px;
        }
      }


      .collection {
        margin-top: 20px;
        padding: 0 10px;
        font-size: 0;

        .empty {
          font-size: $base-font-size;
          text-align: center;
        }
      }

      .collection-header {
        margin-bottom: 40px;
        display: none;
      }

      .collection-featured-image {
        margin-bottom: 20px;

        img {
          display: block;
          margin: 0 auto;
        }
      }

      .collection-description {
        margin: 0 auto 20px;
        max-width: 600px;
        text-align: center;
        font-size: $base-font-size;
      }

      .collection-filters {
        text-align: center;
      }

      .collection-tags-wrapper,
      .collection-sorting {
        display: inline-block;
        max-width: 200px;
        text-align: left;
        margin: 0 10px;
      }


      .template-index {
        .navigation.full-width { border-bottom: 0; }

        .main-content {
          margin-bottom: 65px;
          text-align: center;
          
        }
      }

      .home-slideshow.content-width {
        margin: 0 auto;
        max-width: $max-width;
        padding-left: 30px;
        padding-right: 30px;

        .lt-ie9 & { width: $max-width; }
      }

      .slideshow-slide {
        width: 100%;
      }

      .slide-image-wrapper {
        overflow: hidden;

        img {
          display: block;
          width: 100%;
        }
      }

      $i: 1;
      @while $i < 6 {
        .slide-#{$i} {
          h3,
          p {
            color: nth($slide-colors, $i);
          }
        }
        $i: $i + 1;
      }

      .slide-content {
        position: absolute;
        top: 50%;
        left: 70px;
        right: 70px;
        @include transform(translateY(-50%));

        @include breakpoint(s) {
          left: 30px;
          right: 30px;
        }

        .content-aligned-center & {
          text-align: center;
        }

        .content-aligned-right & {
          text-align: right;
        }
      }

      .slide-content-inner {
        margin: 0 auto;
        max-width: $max-width;
      }

      .slide-title {
        @extend %heading-text;
        margin: 0;
        font-size: 64px;

        {% if settings.heading-font-small-caps %}
        font-size: 60px;
        {% endif %}

        @include breakpoint(m) {
          font-size: 50px;

          {% if settings.heading-font-small-caps %}
          font-size: 45px;
          {% endif %}
        }

        @include breakpoint(s) {
          font-size: 32px;

          {% if settings.heading-font-small-caps %}
          font-size: 28px;
          {% endif %}
        }

        @include breakpoint(xs) {
          font-size: 18px;

          {% if settings.heading-font-small-caps %}
          font-size: 26px;
          {% endif %}
        }
      }

      .slide-subtitle {
        margin: 5px 0 0;
        font-size: 21px;

        @include breakpoint(s) { font-size: 16px; }
        @include breakpoint(xs) { font-size: 14px; }
      }

      .slide-cta {
        margin: 30px 0 0;
        font-size: 16px;

        {% if settings.button-font-small-caps %}
        font-size: 14px;
        {% endif %}

        @include breakpoint(s) { margin-top: 20px; }

        @include breakpoint(xs) {
          margin-top: 15px;
          padding: 10px 15px;
        }
      }

      /* Introduction message */

      .introduction-message-wrap {
        margin: 30px; 
      }

      .introduction-message {
        padding: 30px;

        {% if settings.introduction-message-border %}
        border: 1px solid $border-color;
        {% endif %}
      }

      .introduction-message h2,
      .introduction-message h3 {
        display: block;
        margin: 0 auto 16px;
        max-width: 840px;
      }


      .introduction-message h3 {
        font-family: $body-font;
      }


      /* Featured collections */

      .featured-collections {
        padding: 0 8px;
        font-size: 0;

        > *:first-child {
          margin-top: 0;
        }

        .section-title {
          margin-bottom: 30px;
        }
      }

      .featured-collection {
        display: inline-block;
        vertical-align: top;
        font-size: $base-font-size;
        margin: 0;
        padding: 0 22px;

        .row-of-1 & {
          width: 100%;
        }

        .row-of-2 & {
          width: 50%;

          @include breakpoint(s) {
            width: 100%;
            margin-bottom: 40px;

            &.last {
              margin-bottom: 0;
            }
          }
        }

        .row-of-3 & {
          width: 33.333%;

          @include breakpoint(s) {
            width: 100%;
            margin-bottom: 40px;

            &.last {
              margin-bottom: 0;
            }
          }
        }
      }

      .featured-collection-image {
        position: relative;
        overflow: hidden;

        &:hover {
          .featured-collection-overlay-wrapper {
            &.color-white { background: rgba(0, 0, 0, 0.5); }
            &.color-black { background: rgba(255, 255, 255, 0.5); }
          }

          img { @include transform(scale(1.02)); }
        }

        img {
          -webkit-backface-visibility: hidden;
          @include transition(all 0.2s $ease-out-quad);
        }
      }

      .featured-collection-overlay-wrapper {
        cursor: pointer;
        position: absolute;
        top: 0;
        bottom: 0;
        left: 0;
        right: 0;
        @include transition(background 0.2s $ease-out-quad);
      }

      .featured-collection-overlay {
        position: absolute;
        width: 100%;
        top: 50%;
        left: 50%;
        @include transform(translate(-50%, -50%));

        .color-white & {
          color: #fff;
          text-shadow: 0px 0px 10px rgba(0, 0, 0, 0.3);

          a { color: #fff;
          text-transform: uppercase;
   		  font-size: 32px;
          }
        }

        .color-black & {
          color: #000;
          text-shadow: 0px 0px 10px rgba(255, 255, 255, 0.3);

          a { color: #000; }
        }
      }

      .featured-collection-title {
        margin: 0;
        font-size: 25px;
      }

      .featured-collection-subtitle {
        font-size: 16px;
      }

      .featured-collection-description {
        border: 1px solid $border-color;
        padding: 30px 35px 35px;

        > *:first-child {
          margin-top: 0;
        }

        > *:last-child {
          margin-bottom: 0;
        }

        p {
          margin: 1em 0;
        }
      }

      /* Featured products */

      .featured-products {
        margin-top: 40px;

        .section-title {
          margin-top: 0;
        }
      }

      .featured-products-list {
        padding: 0 10px;
        font-size: 0;
      }

      /* Social widgets */

      .social-widget-wrapper {
        margin: 15px 30px 45px;
        border-top: 1px solid $border-color;
        padding-top: 40px;
        font-size: 0;

        .featured-collections ~ & {
          margin-top: 40px;
        }

        .section-title {
          margin-top: 0;
          text-align: center;
        }
      }

      .social-widget-blurb {
        max-width: 715px;
        margin: 0 auto 40px;
        font-size: $base-font-size;
      }

      .instagram-widget,
      .twitter-widget {
        display: inline-block;
        vertical-align: top;
        font-size: $base-font-size;
      }

      .instagram-widget {
        margin: 0 -5px;
        width: 100%;
        max-height:320px;
        overflow: hidden;
        text-align: center;

        {% if settings.twitter-widget %}
        width: 90%;
        padding-right: 25px;

        @include breakpoint(m) {
          margin-bottom: 40px;
        }
        {% endif %}

        @include breakpoint(m) {
          width: 100%;
          padding: 0;
        }
      }

      .instagram-photo {
        display: inline-block;
        width: 33.333%;
        padding: 0 5px;

        {% if settings.twitter-widget %}
        width: 33.333%;
        {% endif %}

        @include breakpoint(s) {
          width: 33.333%;
          margin-bottom: 4px;
        }
      }

      .instagram-follow {
        margin-top: 20px;
      }

      .twitter-follow {
        margin-top: 25px;
      }

      .twitter-widget {
        max-width: 825px;
        margin: 0 auto;

        {% if settings.instagram-widget %}
        width: 90%;
        padding-left: 25px;
        {% endif %}

        @include breakpoint(m) {
          width: 100%;
          padding: 0;
        }
      }

      .twitter-tweet-wrapper {
        padding: 35px 60px 25px;
        border: 6px solid $border-color;

        .has-instagram & {
          padding: 0 60px;

          @include breakpoint(m) {
            height: auto !important;

            .twitter-tweet {
              padding: 35px 0 !important;
            }
          }
        }

        .tweet {
          margin: 0;
          color: $heading-color;
          font-size: 16px;

          .has-instagram & {
            @include breakpoint(l) {
              font-size: 14px;
            }

            @include breakpoint(m) {
              font-size: 16px;
            }
          }
        }

        .user {
          display: none;
        }

        .timePosted {
          margin-bottom: 0;
          text-transform: uppercase;
        }

        a {
          word-wrap: break-word;
        }
      }

      .twitter-icon {
        font-family: 'Social icons';
        font-size: 17px;
        position: relative;
        top: 1px;
        margin-right: 5px;
      }


      .collections-list {
        margin-top: 20px;
        padding: 0 10px;
        font-size: 0;

        .collection {
          margin-bottom: 0;
          display: inline-block;
          vertical-align: top;
          padding: 0 20px;
          font-size: $base-font-size;
          text-align: center;

          .thumbnail {

            a {
              display: block;
            }

            img {
              display: block;
              margin: 0 auto;
            }
          }

          .collection-title {
            margin-bottom: 10px;

            a {
              color: $heading-color;
            }
          }
        }

        &.rows-of-1 .collection {
          width: 100%;
        }

        &.rows-of-2 .collection {
          width: 50%;

          @include breakpoint(s) {
            width: 100%;
          }
        }

        &.rows-of-3 .collection {
          width: 33.333%;

          @include breakpoint(s) {
            width: 100%;
          }
        }
      }

      .template-page {
        .main-content {
          margin-bottom: 65px;
        }

        .page-content {
          padding: 0 30px;
          max-width: 710px;
          margin: 40px auto 0;

          > *:not(.highlight) {
            max-width: 600px;
            margin-left: auto;
            margin-right: auto;
          }
        }

        .contact-form {
          margin: 40px auto;
          padding: 0 30px;
          max-width: 400px;

          label {
            display: block;
            margin: 32px 0 22px;
          }

          input[type=submit] {
            display: block;
            margin-top: 32px;
          }
        }
      }

      .success-message,
      .error-message {
        margin-top: 25px;
        font-style: italic;
      }


      .template-product {
        .main-content {
          position: relative;
          margin-bottom: 65px;
          font-size: 0;
        }

        .share-buttons {
          /*margin-top: 30px;*/
        }
      }

      .product-wrap {
        @extend %clearfix;
        margin-top: 35px;
      }

      .main-product-wrap {
        position: relative;
        font-size: 18px;

        {% if settings.related-products-position == "right" %}
        width: 88%;
        display: inline-block;
        vertical-align: top;

        @include breakpoint(m) { width: 100%; }
        {% endif %}
      }

      .product-images {
        @extend %clearfix;
        position: relative;
        float: left;
        width: 45%;
        padding: 0 25px 0 30px;

        @include breakpoint(s) {
          width: 100%;
          margin-bottom: 40px;
        }
      }

      .product-main-image {
        position: relative;
        @include transition(height 0.2s $ease-out-quad);

        img {
          z-index: 100;
          display: block;
          margin: 0 auto;
          max-height: 650px;
          @include transition(opacity 0.2s $ease-out-quad);
        }

        &.zoom-enabled {
          cursor: pointer;
          cursor: -webkit-zoom-in;
          cursor: -moz-zoom-in;
          cursor: zoom-in;
        }
      }

      .product-zoom {
        position: absolute;
        background-color: white;
        background-repeat: no-repeat;
        display: none;
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        z-index: 10;

        cursor: pointer;
        cursor: -webkit-zoom-out;
        cursor: -moz-zoom-out;
        cursor: zoom-out;

        &.active {
          display: block;
        }
      }

      .product-thumbnails-outer-wrapper {
        position: relative;
        margin: 20px auto 0;
        font-size: 0;
        text-align: center;
      }

      .product-thumbnails-wrapper {
        display: inline-block;
        vertical-align: middle;
        width: 100%;
        max-width: 80%;
        overflow: hidden;
        position: relative;
      }

      .product-thumbnails {
        margin: 0 auto;
        font-size: 0;
        @include transition(left 0.2s ease-in-out);

        &.has-side-scroll {
          position: absolute;
          left: 0;
          text-align: left;
        }
      }

      .product-thumbnail {
        position: relative;
        display: inline-block;
        width: 25%;
        padding: 0 6px;
        vertical-align: top;
        cursor: pointer;

        &.active:before {
          content: '';
          position: absolute;
          top: 0;
          bottom: 0;
          left: 6px;
          right: 6px;
          border: 2px solid $border-color;
        }

        img {
          display: block;
          margin: 0 auto;
        }
      }

      .product-thumbnails-navigation-previous,
      .product-thumbnails-navigation-next {
        @extend %icon;
