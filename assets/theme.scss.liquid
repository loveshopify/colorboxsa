    &.has-2-features .masonry-feature {
      width: 50%;
      padding-bottom: 30%;

      &.feature-1 {
        @extend %left, %bottom;
      }

      &.feature-2 {
        @extend %right;
        {% unless settings.masonry-features-layout == "content-width" %}
        margin-bottom: 0;
        {% endunless %}
      }
    }

    &.has-3-features .masonry-feature {
      width: 33.333%;
      padding-bottom: 25%;

      &.feature-1 {
        @extend %left, %bottom;
      }

      &.feature-2 {
        @extend %bottom;
      }

      &.feature-3 {
        @extend %right;
        {% unless settings.masonry-features-layout == "content-width" %}
        margin-bottom: 0;
        {% endunless %}
      }
    }

    &.has-4-features {
      .feature-1 {
        @extend %left;
        width: 50%;
        padding-bottom: 20.6%;
      }

      .feature-2 {
        @extend %right;
        width: 50%;
        padding-bottom: 28%;
      }

      .feature-3 {
        @extend %left, %bottom;
        width: 50%;
        padding-bottom: 28%;

        @include breakpoint(s) {
          top: 0 !important;
        }
      }

      .feature-4 {
        @extend %right;
        width: 50%;
        padding-bottom: 20.6%;

        {% unless settings.masonry-features-layout == "content-width" %}
        margin-bottom: 0;
        {% endunless %}
      }
    }

    &.has-5-features {
      .feature-1 {
        @extend %left;
        width: 30%;
        padding-bottom: 20.6%;
      }

      .feature-2 {
        @extend %right;
        width: 70%;
        padding-bottom: 28%;
      }

      .feature-3 {
        @extend %left, %bottom;
        width: 30%;
        padding-bottom: 28%;

        @include breakpoint(s) {
          top: 0 !important;
        }
      }

      .feature-4 {
        @extend %bottom;
        width: 40%;
        padding-bottom: 20.6%;
      }

      .feature-5 {
        @extend %right;
        width: 30%;
        padding-bottom: 20.6%;

        {% unless settings.masonry-features-layout == "content-width" %}
        margin-bottom: 0;
        {% endunless %}
      }
    }

    &.has-6-features {
      .feature-1 {
        @extend %left;
        width: 30%;
        padding-bottom: 20.6%;
      }

      .feature-2 {
        width: 30%;
        padding-bottom: 28%;
      }

      .feature-3 {
        @extend %right;
        width: 40%;
        padding-bottom: 28%;
      }

      .feature-4 {
        @extend %left, %bottom;
        width: 30%;
        padding-bottom: 28%;

        @include breakpoint(s) {
          top: 0 !important;
        }
      }

      .feature-5 {
        @extend %bottom;
        width: 40%;
        padding-bottom: 20.6%;
      }

      .feature-6 {
        @extend %right;
        width: 30%;
        padding-bottom: 20.6%;

        {% unless settings.masonry-features-layout == "content-width" %}
        margin-bottom: 0;
        {% endunless %}
      }
    }

    @include breakpoint(s) {
      margin-bottom: 0 !important;

      .feature-1,
      .feature-2,
      .feature-3,
      .feature-4,
      .feature-5,
      .feature-6 {
        display: none;
        padding: 0;
        width: 100% !important;
        padding-bottom: 0 !important;
        height: auto !important;

        &.enable-mobile-true {
          display: block;
        }
      }
