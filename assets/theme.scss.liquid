      -webkit-font-smoothing: antialiased;

      {% if settings.store-title-font-small-caps %}
      font-size: 20px;
      @extend %small-caps;
      {% endif %}
    }

    img {
      {% if settings.alternative-index-layout-logo-height != blank %}
      {% if settings.alternative-index-layout-logo-height contains 'px' %}
      height: {{ settings.alternative-index-layout-logo-height }};
      {% else %}
      height: {{ settings.alternative-index-layout-logo-height }}px;
      {% endif %}
      {% endif %}
    }
  }

  .alternative-masonry-features {
    font-size: 0;
    overflow: hidden;
    @include transition(opacity 0.2s $ease-out-quad);

    {% if settings.masonry-features-gutters %}
    padding: 5px;
    {% endif %}

    @include breakpoint(s) { overflow: visible; }

    @for $x from 5 through 6 {
      &.has-#{$x}-features {
        @for $i from 1 through 6 {
          .feature-#{$i} {
            @if $x == 5 and $i == 2 {
              width: 70%;
              } @else if $x == 5 and $i == 4 or $x == 6 and $i == 3 or $x == 6 and $i == 5 {
                width: 40%;
                } @else {
                  width: 30%;
                }
              }
            }
          }
        }

        .loading & {
          opacity: 0;
        }
      }

      .alternative-masonry-feature {
        position: relative;
        margin: 0;
        display: inline-block;
        vertical-align: top;
        overflow: hidden;

        {% if settings.masonry-features-gutters %}
        border: 5px solid #fff;
        {% endif %}

        img {
          display: block;
          width: 100%;
          -webkit-backface-visibility: hidden;
          @include transition(all 0.2s $ease-out-quad);
        }

        figure:after {
          content: '';
          position: absolute;
          top: 0;
          bottom: 0;
          left: 0;
          right: 0;
          @include transition(all 0.2s $ease-out-quad);
        }

        .has-animation &:hover {
          img { @include transform(scale(1.02)); }

          &.color-black figure:after { background: rgba(255,255,255,0.5); }
          &.color-white figure:after { background: rgba(0,0,0,0.5); }
        }

        .has-1-features & { width: 100%; }
        .has-2-features & { width: 50%; }
        .has-3-features & { width: 33.333%; }
        .has-4-features & { width: 50%; }

        @include breakpoint(s) {
          &.enable-mobile-false { display: none; }
        }

        &.no-image { background: mix($border-color, $body-background-color, 30%); }

        &.has-link { cursor: pointer; }

        &.mobile {
          width: 100% !important;
          margin-top: 0 !important;
        }

        &.natural {
          height: auto !important;

          &.no-image { height: 200px !important; }
          figure { width: 100% !important; }
        }
      }


